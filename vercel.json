{
  "version": 2,
  "name": "front-petgram-carlosjmarq",
  "builds": [
    {
      "use": "@now/static-build",
      "src": "package.json"
    }
  ],
  "routes": [
    {
      "src": "(.*).js",
      "dest": "/1.js"
    },
    {
      "src": "(.*).json",
      "dest": "/1.json"
    },
    {
      "src": "(.*).css",
      "dest": "/1.css"
    },
    {
      "src": "(.*).svg",
      "dest": "/1.svg"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ]
}
